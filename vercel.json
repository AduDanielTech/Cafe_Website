{
    "version": 2,
    "builds": [
        {
           "src": "./index.js",
           "use": "@vercel/node",
           "config": { "maxLambdaSize": "25mb" },
           "build": {
              "command": "npm install child_process"  
           }
        }
     ],     
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}